<UserControl x:Name="navigation_buttons_control" x:Class="ReferenceDataUploader.Navigation_Buttons"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ReferenceDataUploader"
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="350" MinWidth="350" MinHeight="30">
    <Grid x:Name="navigation_buttons_grid" MinWidth="350" MinHeight="30">
        <Button x:Name="button_finish" Content="Finish" Margin="0" Click="finish_clicked" RenderTransformOrigin="2,-3.25" HorizontalAlignment="Right" Width="100" Height="20" VerticalAlignment="Bottom"/>
        <Button x:Name="button_next" Content="Next" Margin="0,0,105,0" Click="next_clicked" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height="20" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="100"/>
        <Button x:Name="button_previous" Content="Previous" Margin="0,0,210,0" Click="previous_clicked" RenderTransformOrigin="-0.226,-3.25" HorizontalAlignment="Right" Width="100" Height="20" VerticalAlignment="Bottom"/>

    </Grid>
</UserControl>
